    "/v0/usage/rpc": {
      "get": {
        "summary": "Retrieves usage total for specified time range",
        "tags": [
          "Usage"
        ],
        "parameters": [
          {
            "name": "start_time",
            "in": "query",
            "description": "Start time",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "end_time",
            "in": "query",
            "description": "End time",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "description": "If start_time or end_time are missing, current billing period is used and remaining credits for the period are returned.",
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "credits_used": {
                          "type": "integer"
                        },
                        "credits_remaining": {
                          "type": "integer",
                          "nullable": true
                        },
                        "limit": {
                          "type": "integer",
                          "nullable": true
                        },
                        "overages": {
                          "type": "integer",
                          "nullable": true
                        },
                        "start_time": {
                          "type": "integer",
                          "format": "timestamp"
                        },
                        "end_time": {
                          "type": "integer",
                          "format": "timestamp"
                        }
                      },
                      "required": [
                        "credits_used",
                        "start_time",
                        "end_time"
                      ]
                    },
                    "error": {
                      "type": "string",
                      "nullable": true
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
